.page{
    height: 100vh;
    display: flex;
    flex-direction: column;
    background: #F7FAFA;
}
.custom-navbar {
    position: relative;
    top: 0;
    width: 100%;
    z-index: 10;
  }
.content{
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: start;
    overflow: auto;
    /* margin: auto 16px; */
    background: #F7FAFA;
    padding: 16px;
}
.box-container{
    padding: 16px;
    background: #F7FAFA;
}
.box-list{
    display: flex;
    flex-direction: column;
    width: 100%;
    flex-wrap: wrap;
    gap: 20px;
}