.page{
  height: 100vh;
  display: flex;
  flex-direction: column;
  background: #F7FAFA;
}
.custom-navbar {
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 10;
}
.content{
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: start;
  overflow: auto;
  margin: 100px 16px 16px 16px;
  /* margin-bottom: 40px; */
  background: #F7FAFA;
  padding: 20px 0;
}

.maintain-container {
    padding: 16px;
    background: #F7FAFA;
    height: 100vh;
}
.maintain-card{
    padding: 16px;
    border-radius: 6px;
    background: linear-gradient(0deg, rgba(0, 0, 0, 0.001), rgba(0, 0, 0, 0.001)), #FFFFFF;
    box-shadow: 0px 1px 2px 0px rgba(0, 0, 0, 0.05);
}

.picker {
    padding: 10px;
    border: 1px solid #ccc;
    margin-top: 10px;
    text-align: center;
  }
  .field{
    /* background-color: #f5f5f5; */
    width: 100%;
    display: flex;
    flex-direction: row nowrap;
  }

  .btn{
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    margin-bottom: 16px;
  }
  
  .upload-btn{
    width: 100%;
    border-radius: 6px;
    /* background-color: #14B8A6; */
  }

  .upload{
    width: 100%;
    /* display: block; */
  }

  .form-box{
    /* display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center; */
    display: block;
    background-color: #F9FAFB;
    padding: 12px 24px;
    border-radius: 6px;
    margin-top: 12px;
  }

  .test-field{
    display: flex;
    flex-direction: row;
    justify-content: space-around;
  }

  .test-card{
    width: 100%;
    /* background-color: #F9FAFB; */
    padding: 32rpx 24rpx;
    border: 1px solid #ccc;
    border-radius: 6px;
    margin: 24rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    font-size: 32rpx;
  }

  .test-card--active{
    /* border-color: #ccc; */
    background-color: #14B8A6;
    color: #fff;
  }

  .submit-btn{
    margin: 16px;
    border-radius: 6px;
  }